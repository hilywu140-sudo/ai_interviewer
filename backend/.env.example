# ==================== 数据库配置 ====================
DATABASE_URL=postgresql://postgres:password@localhost:5432/ai_interviewer

# ==================== LLM API 配置 ====================
# OpenAI
OPENAI_API_KEY=sk-your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# DeepSeek（推荐用于开发测试）
DEEPSEEK_API_KEY=sk-your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEFAULT_LLM_PROVIDER=deepseek

# 通义千问（Supervisor 节点加速，可选）
# 如果不设置，会自动使用 DASHSCOPE_API_KEY
QWEN_API_KEY=sk-your_qwen_api_key_here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_SUPERVISOR_MODEL=qwen3-8b

# ==================== 阿里云服务配置 ====================
# OSS 对象存储
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret
ALIYUN_OSS_BUCKET=your_oss_bucket_name
ALIYUN_OSS_ENDPOINT=oss-cn-shanghai.aliyuncs.com

# DashScope ASR（语音识别）
DASHSCOPE_API_KEY=sk-your_dashscope_api_key_here

# 阿里云短信（可选）
ALIYUN_SMS_SIGN_NAME=your_sms_sign_name
ALIYUN_SMS_TEMPLATE_CODE=your_sms_template_code

# ==================== 应用配置 ====================
APP_ENV=development
SECRET_KEY=your_secret_key_here_min_32_chars

# JWT 认证（可选，不设置则使用 SECRET_KEY）
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRE_DAYS=7

# CORS 跨域配置
# 开发环境
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
# 生产环境（部署时修改为实际域名）
# CORS_ORIGINS=https://your-app.vercel.app,https://yourdomain.com

# ==================== LangSmith 监控（可选）====================
LANGSMITH_API_KEY=lsv2_pt_your_langsmith_api_key_here
LANGSMITH_PROJECT=ai-interviewer
LANGSMITH_TRACING=true

# ==================== 音频存储 ====================
AUDIO_STORAGE_PATH=./audio_files
